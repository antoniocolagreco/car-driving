{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "paths": {
            "@components/*": ["src/components/*"],
            "@pages/*": ["src/pages/*"],
            "@layouts/*": ["src/layouts/*"],
            "@libs/*": ["src/libs/*"],
            "@css/*": ["src/css/*"],
            "@models/*": ["src/models/*"]
        }
    }
}
